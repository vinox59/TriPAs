{
	"name": "tripas",
	"description": "TriPAs - Make it Simple",
	"version": "0.0.1",
	"publisher": "TriPAs",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/vinox59/TriPAs"
	},
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:catCoding.start",
		"onCommand:catCoding.doRefactor",
		"onCommand:styleCollison.execute",
		"onWebviewPanel:catCoding"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "catCoding.start",
				"title": "Make it Simple",
				"category": "TriPAs"
			},
			{
				"command": "catCoding.doRefactor",
				"title": "Do some refactoring",
				"category": "Cat Coding"
			},
			{
				"command": "styleCollison.execute",
				"title": "Run for Style Collision",
				"category": "TriPAs"
			},
			{
				"command": "launchTFS.execute",
				"title": "Launch TFS",
				"category": "TriPAs"
			},
			{
				"command": "tripasLint.execute",
				"title": "Run Lint",
				"category": "TriPAs"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Tripas Configuration",
			"properties": {
				"tripas.workTime": {
					"type": "number",
					"default": 1,
					"description": "Time of work, in minutes."
				},
				"tripas.pauseTime": {
					"type": "number",
					"default": 0.5,
					"description": "Time of pause, in minutes"
				},
				"tripas.repeat": {
					"type": "number",
					"default": 2,
					"description": "Number of repeat before long break"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -w -p ./"
	},
	"dependencies": {},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.47.0",
		"@types/vscode-webview": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"mocha": "^6.2.0",
		"eslint": "^7.21.0",
		"typescript": "4.1.6"
	}
}
